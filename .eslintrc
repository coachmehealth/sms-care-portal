{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "extends": ["airbnb-typescript/base", "prettier"],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-console": "warn",
    "max-len": "off",
    "no-underscore-dangle": "off",
    "prettier/prettier": "error",
  }
}
